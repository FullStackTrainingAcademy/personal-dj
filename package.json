        "cors": "^2.8.4",
        "express": "^4.17.1",
        "helmet": "^4.1.1",
        "jsonwebtoken": "^8.5.1",
        "node-localstorage": "^2.1.6",
        "nodemon": "^2.0.4",
        "querystring": "^0.2.0",
        "request": "^2.88.2"
    },
    "engines": {
        "node": "12.0.0"
    },
    "devDependencies": {
        "husky": "^4.3.0",
        "prettier": "2.1.2",
